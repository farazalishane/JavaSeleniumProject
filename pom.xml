	<!--  -->
	
	<!--  -->
	<name>ChRL - Spring / Vaadin Servlet</name>
	<description>Generated by ChRL Spring Sample</description>
	<!--  -->
	
	<!--  -->
	<properties>
		<server.deployment.tomcat>http://localhost:8080</server.deployment.tomcat>
		<chrlVersion>${chrlArchtypeVersion}</chrlVersion>
	</properties>
	<!--  -->
	
	<!--  -->
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>at.chrl</groupId>
				<artifactId>chrl-bom</artifactId>
				<version>${chrlVersion}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<!--  -->
	
	<!--  -->
	<dependencies>
		<!-- Spring -->
		<dependency>
			<groupId>at.chrl</groupId>
			<artifactId>chrl-orm-spring-integration</artifactId>
		</dependency>
		<!-- Servlet -->
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
